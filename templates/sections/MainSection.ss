<main class="section main-section" id="main" role="main" tabindex="3">
	<% if Title %>
	<header class="header" >
		<h1 class='title'>
			$Title
		</h1>
	</header>
	<% end_if %>
	<% if Content %>
	<div class='content'>
		$Content
	</div>
	<% end_if %>
	<% if Form %>
	<div class="form">
		$Form
	</div>
	<% end_if %>
</main>
